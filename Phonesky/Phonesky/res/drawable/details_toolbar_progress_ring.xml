<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="ring" android:innerRadiusRatio="2.3" android:thicknessRatio="33.0" android:useLevel="false">
            <solid android:color="@color/d30_toolbar_progress_ring_color" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <rotate android:fromDegrees="-90.0" android:toDegrees="-90.0">
            <shape android:shape="ring" android:innerRadiusRatio="2.3" android:thicknessRatio="18.0" android:useLevel="true">
                <solid android:color="@color/green_500" />
            </shape>
        </rotate>
    </item>
</layer-list>