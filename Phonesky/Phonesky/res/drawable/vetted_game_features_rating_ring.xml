<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="ring" android:useLevel="false" android:innerRadius="@dimen/vetted_game_feature_rating_inner_radius" android:thickness="@dimen/vetted_game_feature_rating_ring_thickness">
            <solid android:color="@color/vetted_game_feature_rating_bg" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <rotate android:fromDegrees="@dimen/vetted_game_feature_rating_rotation" android:toDegrees="@dimen/vetted_game_feature_rating_rotation">
            <shape android:shape="ring" android:useLevel="true" android:innerRadius="@dimen/vetted_game_feature_rating_inner_radius" android:thickness="@dimen/vetted_game_feature_rating_ring_thickness">
                <solid android:color="@color/vetted_game_feature_rating_fg" />
            </shape>
        </rotate>
    </item>
</layer-list>